<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="pnExpirationTimePopup" prefHeight="200.0" prefWidth="500.0" style="-fx-background-color: #87919b;" stylesheets="@Styles.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.popupExpirationTimeController">
   <children>
      <Label styleClass="label-title" stylesheets="@Styles.css" text="SUA LICENÇA EXPIROU!" StackPane.alignment="CENTER">
         <StackPane.margin>
            <Insets bottom="100.0" />
         </StackPane.margin>
      </Label>
      <Label styleClass="label-text" stylesheets="@Styles.css" text="ATIVE O SUPORTE PARA INSERIR UMA NOVA &#10;CHAVE DE ATIVAÇÃO " textAlignment="CENTER" wrapText="true">
         <StackPane.margin>
            <Insets top="30.0" />
         </StackPane.margin>
      </Label>
      <Button mnemonicParsing="false" onAction="#btnOk" styleClass="button-action" text="OK" StackPane.alignment="BOTTOM_RIGHT">
         <StackPane.margin>
            <Insets bottom="10.0" right="10.0" />
         </StackPane.margin>
      </Button>
   </children>
</StackPane>
