<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="pnAccessPopup" prefHeight="200.0" prefWidth="500.0" style="-fx-background-color: #87919b;" stylesheets="@Styles.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.popupAcessKeyController">
   <children>
      <Button mnemonicParsing="false" onAction="#btnApplyAction" prefHeight="50.0" prefWidth="143.0" styleClass="button-action" text="APLICAR" StackPane.alignment="BOTTOM_RIGHT">
         <StackPane.margin>
            <Insets bottom="20.0" right="20.0" />
         </StackPane.margin>
      </Button>
      <Label styleClass="label-title" stylesheets="@Styles.css" text="INSIRA A CHAVE DE LICENÇA" StackPane.alignment="CENTER">
         <StackPane.margin>
            <Insets bottom="100.0" right="130.0" />
         </StackPane.margin>
      </Label>
      <TextField fx:id="txtkey" prefHeight="25.0" prefWidth="441.0">
         <StackPane.margin>
            <Insets left="20.0" right="30.0" />
         </StackPane.margin>
      </TextField>
      <Label fx:id="lblIncorrectKey" styleClass="label-title" stylesheets="@Styles.css" text="CHAVE INVÁLIDA" visible="false">
         <StackPane.margin>
            <Insets right="130.0" top="100.0" />
         </StackPane.margin>
      </Label>
   </children>
</StackPane>
